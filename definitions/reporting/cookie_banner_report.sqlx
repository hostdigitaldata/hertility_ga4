config {
    type: "table"

}
SELECT
  event_date AS date,
  FORMAT_DATE('%A', PARSE_DATE('%Y%m%d', event_date)) AS day_of_week,
  COUNT(DISTINCT user_pseudo_id) AS total_users
FROM ${ref(constants.GA4_TABLE)}
WHERE
  _TABLE_SUFFIX BETWEEN '20240201' AND '20240213'
GROUP BY
  event_date
ORDER BY
  event_date
