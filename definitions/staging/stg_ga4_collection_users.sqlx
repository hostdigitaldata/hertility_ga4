config {
  type: "view", // Creates a view in BigQuery. Try changing to "table" instead.
}

SELECT
   date,
   device,
   stitched_user_id,
   stitched_user_id AS collection_users,
   page
FROM ${ ref('base_ga4_pages') }
WHERE page like '%collections/build-a-box%'

GROUP BY 1, 2, 3, 4,5