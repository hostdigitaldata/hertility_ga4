  config {
  type: "view", // Creates a view in BigQuery. Try changing to "table" instead.
}
  
  
  
  select
       date,
       device,
       stitched_user_id,
       purchasers,
       max(engaged_sessions) as is_engaged,
       sum(revenue) as revenue,
       sum(transactions) as transactions
from ${ ref ('base_ga4_traffic_conversions') }
group by 1,2,3,4