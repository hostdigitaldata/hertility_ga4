config {
  type: "table", // Creates a view in BigQuery. Try changing to "table" instead.
}

select
   event_timestamp,
   session_id,
   user_pseudo_id,
   user_id,
   device,
   operating_system,
   browser,
   event_name

from ${ ref('base_ga4_events') }
where event_name in ( 'quiz_start', 'quiz_finish')

group by all