config {
  type: "view", // Creates a view in BigQuery. Try changing to "table" instead.
}

select
   date,
   device,
   source_medium,
   stitched_user_id,
   stitched_user_id AS begin_checkout_users

from ${ ref('base_ga4_events') }
where event_name = 'add_to_cart'

group by 1, 2, 3, 4